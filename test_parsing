////////parsing :

minishell$ echo $"$HOME"
$$HOME

minishell$ echo $HO'ME'
$HOME

minishell$ echo "$"$HOME
$$HOME

minishell$ echo "'$HOME'"
$HOME

minishell$ echo '"'$HOME'"'
"$HOME"
export var="hello             world"
var="hello             world"
minishell$ echo $var
bash-3.2$ export var="hello      world"
bash-3.2$ echo $var
hello world


bash-3.2$ export var="hello      world"
bash-3.2$ echo $var
hello world
bash-3.2$ echo "$var"
hello      world
bash-3.2$ 


minishell$ cat << m
> $HOME
> "$HOME"
> '$HOME'
> m
/mnt/homes/arahhabE
"/mnt/homes/arahhabME"
'/mnt/homes/arahhabME'
minishell$ cat << m"

minishell$ cat << "m"
> $HOME
> "$HOME"
> '$HOME'
> m
/mnt/homes/arahhabE
"/mnt/homes/arahhabME"
'/mnt/homes/arahhabME'

