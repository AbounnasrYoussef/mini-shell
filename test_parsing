////////parsing :

✅minishell$ cat << file
> $HOME
> file
/mnt/homes/arahhabE

minishell$ cat << file
> "$HOME"
> file
"/mnt/homes/arahhabME"

minishell$ cat << file
> ''"'$HOME'"''
file


✅minishell$ echo $"$HOME"
$$HOME

minishell$ echo $HO'ME'
$HOME


✅minishell$ echo "'$HOME'"
$HOME

minishell$ echo '"'$HOME'"'
"$HOME"

✅minishell$ echo "'$HO'M'E'"
$HOME

bash-3.2$ ''"'$HOME'"''
bash: '/mnt/homes/arahhab': No such file or directory

minishell$ $H"OME"
---- Commande 1 ----
cmd[0] = '/mnt/homes/arahhab'

minishell$ $"HOME"
---- Commande 1 ----
cmd[0] = '/mnt/homes/arahhab'

minishell$ $""
---- Commande 1 ----
cmd[0] = '$'

export var="hello             world"
var="hello             world"
minishell$ echo $var
bash-3.2$ export var="hello      world"
bash-3.2$ echo $var
hello world

bash-3.2$ export var="hello      world"
bash-3.2$ echo $var
hello world
bash-3.2$ echo "$var"
hello      world
bash-3.2$ 


minishell$ cat << "m"
> $HOME
> "$HOME"
> '$HOME'
> m
/mnt/homes/arahhabE
"/mnt/homes/arahhabME"
'/mnt/homes/arahhabME'


