minishell$ echo '"'$HOME'"'
"$HOME"
export var="hello             world"
var="hello             world"
minishell$ echo $var
bash-3.2$ export var="hello      world"
bash-3.2$ echo $var
hello world


bash-3.2$ export var="hello      world"
bash-3.2$ echo $var
hello world
bash-3.2$ echo "$var"
hello      world
bash-3.2$ 


